import java.util.Scanner;

public class Solution {

    public static void main(String[] args) {
            Scanner sc=new Scanner(System.in);
            System.out.println("================================");
            for(int i=0;i<3;i++){
                String s1=sc.next();
                int x=sc.nextInt();
            System.out.printf("%s%03d\n",String.format("%0$-15s",s1),x);   
            }
            System.out.println("================================");

    }
}


